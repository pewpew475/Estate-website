{
  "version": 2,
  "builds": [
    { "src": "home.html", "use": "@vercel/static" },
    { "src": "styles.css", "use": "@vercel/static" },
    { "src": "script.js", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(styles.css|script.js)", "dest": "/$1" },
    { "src": "/(.*)", "dest": "/home.html" }
  ]
}
